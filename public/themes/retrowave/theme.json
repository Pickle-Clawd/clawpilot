{
  "id": "retrowave",
  "name": "Retrowave",
  "author": "Clawd",
  "description": "Neon-soaked synthwave nightdrive",
  "preview": ["#0D0221", "#FF2E97", "#00F0FF"],
  "variables": {
    "--background": "#0D0221",
    "--foreground": "#F0E6FF",
    "--card": "#1A0533",
    "--card-foreground": "#F0E6FF",
    "--popover": "#1A0533",
    "--popover-foreground": "#F0E6FF",
    "--primary": "#FF2E97",
    "--primary-foreground": "#FFFFFF",
    "--secondary": "#190440",
    "--secondary-foreground": "#F0E6FF",
    "--muted": "#190440",
    "--muted-foreground": "#8B7FAA",
    "--accent": "#1F0845",
    "--accent-foreground": "#F0E6FF",
    "--destructive": "#FF1744",
    "--border": "rgba(255, 46, 151, 0.15)",
    "--input": "rgba(255, 46, 151, 0.12)",
    "--ring": "#FF2E97",
    "--chart-1": "#FF2E97",
    "--chart-2": "#00F0FF",
    "--chart-3": "#FF6B35",
    "--chart-4": "#B026FF",
    "--chart-5": "#FFE744",
    "--gradient-orange": "#FF6B35",
    "--gradient-pink": "#FF2E97",
    "--gradient-purple": "#B026FF",
    "--success": "#00FF88",
    "--warning": "#FFE744",
    "--sidebar": "#0A0118",
    "--sidebar-foreground": "#F0E6FF",
    "--sidebar-primary": "#FF2E97",
    "--sidebar-primary-foreground": "#FFFFFF",
    "--sidebar-accent": "#1F0845",
    "--sidebar-accent-foreground": "#F0E6FF",
    "--sidebar-border": "rgba(255, 46, 151, 0.12)",
    "--sidebar-ring": "#FF2E97"
  },
  "ambient": {
    "particles": {
      "count": 50,
      "color": "rgba(255, 46, 151, {{opacity}})",
      "glowColor": "rgba(255, 46, 151, {{glowOpacity}})",
      "highlightColor": "rgba(0, 240, 255, {{highlightOpacity}})",
      "minSize": 1,
      "maxSize": 5,
      "minDuration": 6,
      "maxDuration": 18,
      "direction": "up",
      "wobble": true,
      "maxWobble": 50,
      "glow": true
    }
  }
}
